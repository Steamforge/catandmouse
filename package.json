{
  "name": "cat-and-mouse",
  "description": "A card game known as 'Cat and Mouse' or 'Spite and Malice'",
  "version": "0.1.0",
  "scripts": {
    "build": "gatsby build",
    "build-debug": "gatsby build --no-uglify --verbose --log-pages",
    "dev": "gatsby develop -H 0.0.0.0 -p 8000 -o",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run dev",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint \"./src/**\" -o ./test/eslintreport.html -f html",
    "lint-fix": "eslint \"src/**\" -o ./test/eslintreport.html -f html --fix",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "gatsby": "^3.2.1",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-sass": "^4.2.0",
    "gatsby-plugin-webfonts": "^2.1.0",
    "lodash": "^4.17.21",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-react-app": "^6.0.0",
    "prettier": "^2.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Steamforge/catandmouse"
  },
  "bugs": {
    "url": "https://github.com/Steamforge/catandmouse/issues"
  }
}
